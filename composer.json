{
    "name": "syntaxevolution/laravel-permission",
    "description": "Permission handling for Laravel 5.8 and up with support for tenancy at tenant level",
    "keywords": [
        "spatie",
        "laravel",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security",
        "tenancy"
    ],
    "homepage": "https://github.com/syntaxevolution/laravel-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "Topher Lazarz",
            "email": "topher@syntaxevolution.com",
            "homepage": "https://www.syntaxevolution.com",
            "role": "Contributor"
        }
    ],
    "require": {
        "php" : ">=7.2",
        "hyn/multi-tenant": ">=5.5",
        "illuminate/auth": ">=7.0",
        "illuminate/container": ">=7.0",
        "illuminate/contracts": ">=7.0",
        "illuminate/database": ">=7.0",
        "syntaxevolution/rememberable": ">=4.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "SyntaxEvolution\\Permission\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SyntaxEvolution\\Permission\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SyntaxEvolution\\Permission\\PermissionServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
